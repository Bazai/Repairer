var id = -1;
var stop = false;

<% if @clear.include?("car_model") %>
    var val = $("#car_models option:selected").val();
    $("#car_model_view").html('<%= escape_javascript(render "car_models_ajax/index") %>');
    if (val == null) stop = true;
<% end %>

<% if @clear.include?("production_year") %>
    var val = $("#production_years option:selected").val()
    if (!stop) $("#production_year_view").html('<%= escape_javascript(render "production_years_ajax/index") %>');
    if (val == null) stop = true;
<% end %>

<% if @clear.include?("maintenance") %>
    var val = $("#maintenances option:selected").val()
    if (!stop) $("#maintenance_view").html('<%= escape_javascript(render "maintenances_ajax/index") %>');
    if (val == null) stop = true;
<% end %>

<% if @clear.include?("labor") %>
    if (!stop) $("#labor_view").html('<%= escape_javascript(render "labors_ajax/index") %>');
<% end %>

<% if @clear.include?("part") %>
    if (!stop) $("#part_view").html('<%= escape_javascript(render "parts_ajax/index") %>');
<% end %>
